{
  "non-interactive": true,
  "verbose": true,
  "commitMessage": "chore(release): %s",
  "tagName": "%s",
  "tagAnnotation": "Release %s",
  "buildCommand": "npm run test && npm run changelog && git add CHANGELOG.md",
  "changelogCommand": "conventional-changelog -p angular",
  "requireCleanWorkingDir": true,
  "npm": {
    "publish": true
  },
  "dist": {
    "baseDir": null
  },
  "github": {
    "release": true,
    "tokenRef": "GITHUB_TOKEN"
  }
}
